  "scripts": {
    "nx": "nx",
    "nx:rest": "nx reset",
    "nx:daemon:rest": "nx reset",
    "nx:daemon:help": "nx daemon --help",
    "start": "nx serve",
    "start:api": "nx serve api",
    "start:worker": "nx serve jetstream-worker",
    "start:landing": "nx serve landing",
    "start:docs": "nx serve docs",
    "start:ui:storybook": "nx run ui:storybook",
    "start:auth": "cd apps/authentication && docker-compose up",
    "start:prod": "pm2-runtime start ecosystem.config.js --env production",
    "start:prod:debug": "pm2-runtime start ecosystem.config.js --env production --node-args=\"--inspect\"",
    "start:cron:inactive-account-warning": "node dist/apps/cron-tasks/inactive-account-warning.js",
    "start:cron:inactive-account-deletion": "node dist/apps/cron-tasks/inactive-account-deletion.js",
    "build": "cross-env NODE_ENV=production npm-run-all db:generate build:client build:api build:sw build:landing build:docs rollbar:upload-sourcemaps",
    "build:test": "npm-run-all db:generate build:client:test build:api:test build:sw build:landing build:docs",
    "build:client": "nx build jetstream --prod",
    "build:client:stats": "nx build jetstream --prod --statsJson",
    "build:client:test": "nx build jetstream --configuration=test --verbose",
    "build:ui": "nx build ui --prod",
    "build:ui:test": "nx build ui --configuration=test",
    "build:api": "nx build api --prod",
    "build:cron": "nx build cron-tasks --prod",
    "build:api:test": "nx build api --configuration=test",
    "build:sw": "nx build download-zip-sw --prod",
    "build:landing": "NODE_ENV=production nx run landing:export && npm run build:landing:sitemap",
    "build:landing:sitemap": "next-sitemap",
    "build:docs": "USE_SIMPLE_CSS_MINIFIER=true nx build docs",
    "build:storybook": "build-storybook -c libs/ui/.storybook -o dist/storybook -s node_modules/@salesforce-ux/design-system/assets/styles",
    "bundle-analyzer:client": "npx webpack-bundle-analyzer dist/apps/jetstream/stats.json",
    "docs:index": "docker run -it -e APPLICATION_ID=21D7I5RB7N -e API_KEY=5a8e69d756ddc4a3067c7d9e7aecfc3b -e \"CONFIG=$(cat ./apps/docs/algolia-config.json | jq -r tostring)\" algolia/docsearch-scraper",
    "rollbar:upload-sourcemaps": "ts-node ./scripts/upload-source-maps.ts",
    "generate:data-table-css": "nx workspace-generator convert-sass-to-css ui --filename /src/lib/data-table/data-table-styles.scss",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "chmod +x prisma/seed.sh && prisma db seed",
    "icons:build": "npm-run-all icons:build:*",
    "icons:build:action": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/action ./node_modules/@salesforce-ux/design-system/assets/icons/action",
    "icons:build:custom": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/custom ./node_modules/@salesforce-ux/design-system/assets/icons/custom",
    "icons:build:doctype": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/doctype ./node_modules/@salesforce-ux/design-system/assets/icons/doctype",
    "icons:build:standard": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/standard ./node_modules/@salesforce-ux/design-system/assets/icons/standard",
    "icons:build:utility": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/utility ./node_modules/@salesforce-ux/design-system/assets/icons/utility",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "workspace-generator": "nx workspace-generator"
  },
