  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "start:api": "nx serve api",
    "start:landing": "nx serve landing",
    "start:ui:storybook": "nx run ui:storybook",
    "start:auth": "cd apps/authentication && docker-compose up",
    "start:prod": "pm2-runtime start ecosystem.config.js --env production",
    "start:prod:debug": "pm2-runtime start ecosystem.config.js --env production --node-args=\"--inspect\"",
    "build": "cross-env NODE_ENV=production npm-run-all build:client build:api build:landing",
    "build:test": "npm-run-all build:client:test build:api:test build:landing",
    "build:client": "nx build jetstream --prod",
    "build:client:test": "nx build jetstream --configuration=test --verbose",
    "build:ui": "nx build ui --prod",
    "build:ui:test": "nx build ui --configuration=test",
    "build:api": "nx build api --prod",
    "build:api:test": "nx build api --configuration=test",
    "build:landing": "NODE_ENV=production nx run landing:export",
    "build:storybook": "build-storybook -c libs/ui/.storybook -o dist/storybook -s node_modules/@salesforce-ux/design-system/assets/styles",
    "icons:build": "npm-run-all icons:build:*",
    "icons:build:action": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/action ./node_modules/@salesforce-ux/design-system/assets/icons/action",
    "icons:build:custom": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/custom ./node_modules/@salesforce-ux/design-system/assets/icons/custom",
