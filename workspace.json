    "jetstream": {
      "root": "apps/jetstream",
      "sourceRoot": "apps/jetstream/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/jetstream",
            "index": "apps/jetstream/src/index.html",
            "main": "apps/jetstream/src/main.tsx",
            "polyfills": "apps/jetstream/src/polyfills.ts",
            "tsConfig": "apps/jetstream/tsconfig.app.json",
            "assets": ["apps/jetstream/src/favicon.ico", "apps/jetstream/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/jetstream/src/environments/environment.ts",
                  "with": "apps/jetstream/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "jetstream:build",
            "proxyConfig": "apps/jetstream/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "jetstream:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/jetstream/.eslintrc",
            "tsConfig": ["apps/jetstream/tsconfig.app.json", "apps/jetstream/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/jetstream/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/jetstream/jest.config.js",
            "tsConfig": "apps/jetstream/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
