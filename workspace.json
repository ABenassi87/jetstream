    "jetstream": {
      "root": "apps/jetstream",
      "sourceRoot": "apps/jetstream/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/jetstream",
            "index": "apps/jetstream/src/index.html",
            "main": "apps/jetstream/src/main.tsx",
            "polyfills": "apps/jetstream/src/polyfills.ts",
            "tsConfig": "apps/jetstream/tsconfig.app.json",
            "assets": ["apps/jetstream/src/favicon.ico", "apps/jetstream/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/jetstream/src/environments/environment.ts",
                  "with": "apps/jetstream/src/environments/environment.prod.ts"
                },
                {
                  "replace": "libs/ui/.storybook/storybook-styles.scss",
                  "with": "apps/jetstream/src/main.scss"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "test": {
              "fileReplacements": [
                {
                  "replace": "apps/jetstream/src/environments/environment.ts",
                  "with": "apps/jetstream/src/environments/environment.test.ts"
                },
                {
                  "replace": "libs/ui/.storybook/storybook-styles.scss",
                  "with": "apps/jetstream/src/main.scss"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": false,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "15mb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "jetstream:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "jetstream:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/jetstream/**/*.js",
              "apps/jetstream/**/*.jsx",
              "apps/jetstream/**/*.ts",
              "apps/jetstream/**/*.tsx",
              "apps/jetstream/**/*.spec.ts",
              "apps/jetstream/**/*.spec.tsx",
              "apps/jetstream/**/*.spec.js",
              "apps/jetstream/**/*.spec.jsx",
              "apps/jetstream/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/jetstream/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/jetstream"]
        }
      }
